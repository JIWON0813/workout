scalar DateTime
scalar Date

type Query{
    users : [User]
    user : User
    records : [Record]
    recordsByDay(date : Date) : [Record]
    seeds : Int
}

type Mutation{
    createUser(user : UserInput) : Boolean
    quitUser : Boolean

    sendMessageToken(message : FcmMessageInput) : Boolean

    addRecord(record : RecordInput) : RecordResponse
}