type User{
    id : String!
    name : String
}

type Query{
    users : [User]
    getUser(name : String) : User
}

type Mutation{
    saveUser(user : UserInput) : User
    quitUser(id : String) : Boolean
    sendMessageToken(message : MessageRequest) : Boolean
}

input UserInput {
    id : String
    name : String
}

input MessageRequest {
    title : String
    body : String
    targetToken : String
}

